<div class="search-bar">
  <div class="section-search search-style-2">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12">
          <div class="clearfix">
            <form>
              <section class="container">
                <h3 class="carlist-name">{{carlist.name}}</h3>
                <section class="flex-row justify-center">
                  {{#each mData as |cars|}}
                  {{!-- {{> car-details}} --}}
                  <div class="row align-center mb-5">
                    <h2>{{cars.heading}}</h2>
                    <div class="col-md-6">
                      <h5>Make: {{cars.build.make}}</h5>
                      <h5>Model: {{cars.build.model}}</h5>
                      <div>
                        {{#each cars.media.photo_links as |pic|}}
<img src="{{pic}}" width="150" height="150"/>
                        {{/each}}
                      </div>
                    </div>
                    <div class="col-md-6">
                      <h4>Class Size: {{cars.class}}</h4>
                      <h4>Highway MPG: {{cars.highway_mpg}}</h4>
                      <h4>Highway/City MPG: {{cars.combination_mpg}}</h4>
                      <h4>Displacement: {{cars.displacement}}</h4>
                      <h4>Drive: {{cars.drive}}</h4>
                      <h4>Fuel Type: {{cars.highway_mpg}}</h4>
                      <h4>Transmission: {{cars.transmission}}</h4>
                      <h4>Year: {{cars.year}}</h4>
                    </div>
                  </div>
                  <br>
                  <br>
                  {{/each}}
                </section>
              </section>
              {{!--
            </form> --}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div>

</div>